		<![CDATA[select rownum as rbr, a.* from (
				select t1.id_gn_item_orders_type, t2.d_order, t1.id_gn_item_order, t1.id_gn_org_docs, t3.name, t3.notes, t1.kolicina, t1.iznos, t2.id_status, t4.naziv as status, t2.id_narucitelj, t5.id, t5.naziv, t6.name1, t1.id_status as id_status2,  id_dobavljac, t7.name1 as naziv_org, t1.serijski_broj_od, t1.serijski_broj_do, t3.price
				from vis_ex.gn_item_orders_type t1
				left join vis_ex.gn_item_orders t2 on (t1.id_gn_item_order = t2.id_order)
				left join vis_ex.gn_docs t3 on (t1.id_gn_org_docs = t3.id_gn_org_docs)
				left join vis_ex.gn_item_order_status t4 on (t2.id_status = t4.id)
				left join vis_ex.gn_measure_unit t5 on (t3.id_measure_unit = t5.id)
				left join sm.sm_organizations t6 on (t2.id_narucitelj = t6.id_organization)
