	<queryString>
		<![CDATA[select t1.id_certificate, listagg(t2.name,', ') within group (order by t1.id) as jezici, t3.TEXT as naziv_cert, t3.code, t3.sm_states_id, t4.code as drzava_code, t4.TEXT as drzava, t3.id_measure, t5.name as mjera, t3.status, t3.id_type, t6.naziv as tip, t3.d_change, t3.valid_from, t3.valid_to, t3.notes, t3.document_name, t3.serial_no, t3.serija, t3.price
					from vis_ex.cert_certificates t3
					left join vis_ex.cert_cert_lang t1 on (t1.id_certificate = t3.id)
					left join sm.sm_languages t2 on (t1.id_language = t2.id_language)
					left join sm.sm_states t4 on (t3.sm_states_id = t4.id)
					left join vis_ex.cert_measure t5 on (t3.id_measure = t5.id_measure)
					left join vis_ex.cert_type t6 on (t3.id_type = t6.id_type)
					where t1.activity = 1 and t3.activity = 1
					group by t1.id_certificate, t3.TEXT, t3.code, t3.sm_states_id, t4.code, t4.TEXT, t3.id_measure, t5.name, t3.status, t3.id_type, t6.naziv, t3.d_change, t3.valid_from, t3.valid_to, t3.notes, t3.document_name, t3.serial_no, t3.serija, t3.price
